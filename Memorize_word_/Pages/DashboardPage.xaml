<Page x:Class="Memorize_word_.Pages.DashboardPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Memorize_word_.Pages"
      mc:Ignorable="d" 
      x:Name="dashbord"
      d:DesignHeight="600" d:DesignWidth="800"
      Background="{DynamicResource PrimaryBackground}"
      Title="DashboardPage">

    <Grid>
        <Grid>
            <Border Margin="10" CornerRadius="7" Background="{DynamicResource SecondaryBorderColor}">
                <TextBox Height="150" Width="500" Background="{DynamicResource SecondaryBorderColor}" FontSize="85" Text="Dashboard" FontWeight="DemiBold" FontFamily=" " FontStyle="Oblique" TextChanged="TextBox_TextChanged"/>
            </Border>
        </Grid>
    </Grid>
</Page>
